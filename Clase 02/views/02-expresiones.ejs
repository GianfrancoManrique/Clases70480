<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
<script type="text/javascript">
	$(document).ready(function(){

		$('#btnValidar1').click(function(){

			var regex=/Mar/i;
			var val01="";

			val01=$('#txt01').val();
			if(regex.test(val01)){
				alert(regex.exec(val01));
			}
		})
		
